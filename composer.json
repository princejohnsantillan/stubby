{
    "name": "princejohnsantillan/stubby",
    "description": "Simple way to create files from stubs.",
    "keywords": ["laravel", "stubs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Prince John Santillan",
            "email": "mail@princejohnsantillan.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^9.1",
        "nunomaduro/termwind": "^1.10"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/stubby"]
}
